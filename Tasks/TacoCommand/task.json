{
    "id": "357dac60-72af-11e5-b4ae-29f3a0528042",
    "name": "TacoCliCommand",
    "friendlyName": "TACO CLI Command",
    "description": "Run a TACO CLI Command",
    "author": "Microsoft Corporation",
    "helpMarkDown": "[More Information](http://go.microsoft.com/fwlink/?LinkID=692095)",
    "category": "Build",
    "visibility": [
        "Build"
    ],
    "demands": [
        "npm"
    ],
    "version": {
        "Major": "0",
        "Minor": "1",
        "Patch": "7"
    },
    "minimumAgentVersion": "1.83.0",
    "instanceNameFormat": "TacoCommand $(message)",
    "inputs": [
        {
            "name": "cwd",
            "type": "filePath",
            "label": "Working Directory",
            "defaultValue": "",
            "required": false,
            "helpMarkDown": "Current working directory when TacoCommand is run."
        },
        {
            "name": "tacoCommand",
            "type": "string",
            "label": "Command",
            "defaultValue": "",
            "required": true,
            "helpMarkDown": "TACO command to run. Ex: \"platform add android\""
        },
        {
            "name": "tacoArgs",
            "type": "string",
            "label": "Arguments",
            "required": false,
            "helpMarkDown": "Arguments that should be used for the command."
        },
        {
            "name": "tacoVersion",
            "type": "string",
            "label": "Version",
            "required": false,
            "helpMarkDown": "Version of taco-cli to run command against."
        }
    ],
    "execution": {
        "Node": {
            "target": "taco-command-task.js",
            "argumentFormat": ""
        },
        "PowerShell": {
            "target": "$(currentDirectory)\\bootstrapper.ps1",
            "argumentFormat": "",
            "workingDirectory": "$(currentDirectory)"
        }
    }
}